{
  "name": "nakisanzedaisy.github.io",
  "version": "1.0.0",
  "description": "Personal portfolio website showcasing backend development skills and professional experience",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc && vite build",
    "preview": "vite preview",
    "start": "npm run preview",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix && stylelint '**/*.css' --ignore-path .gitignore",
    "format": "prettier --write '**/*.{html,css,js,json,ts,tsx,md}'",
    "optimize-images": "imagemin images/**/*.{jpg,jpeg,png} --out-dir=images/optimized --plugin=mozjpeg --plugin=pngquant",
    "clean": "rm -rf dist .cache",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests specified yet'"
  },
  "keywords": [
    "portfolio",
    "backend-developer",
    "software-engineering",
    "web-development",
    "static-site",
    "github-pages"
  ],
  "author": "Nakisanze Deziranta",
  "license": "MIT",
  "homepage": "https://nakisanzedaisy.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/nakisanzedaisy/nakisanzedaisy.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/nakisanzedaisy/nakisanzedaisy.github.io/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/material": "^6.1.10",
    "@mui/icons-material": "^6.1.10",
    "framer-motion": "^11.3.28",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^6.1.1",
    "htmlhint": "^1.1.4",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not ie 11"
  ],
  "files": [
    "index.html",
    "style.css",
    "images/",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
